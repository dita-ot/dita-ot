      <xs:attributeGroup ref="copyrfirst.attributes"/>
   </xs:complexType>
   <xs:group name="copyrfirst.content">
      <xs:sequence>
         <xs:group ref="year"/>
      </xs:sequence>
   </xs:group>
   <xs:attributeGroup name="copyrfirst.attributes">
      <xs:attributeGroup ref="data-element-atts"/>
      <xs:attributeGroup ref="global-atts"/>
   </xs:attributeGroup>
   <xs:element name="copyrlast">
      <xs:annotation>
         <xs:documentation>The &lt;copylast&gt; element contains the last copyright year within a multiyear copyright statement. Category: Bookmap elements</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="copyrlast.class">
               <xs:attribute ref="class" default="- topic/data bookmap/copyrlast "/>
