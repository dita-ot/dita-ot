<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE topic PUBLIC "-//OASIS//DTD DITA 1.2 Topic//EN" "topic.dtd">
<topic id="a">
  <title>Keyref</title>
  <related-links>
    <link keyref="empty"/>
  </related-links>
  <topic id="nolink">
    <title>Without href</title>
    <prolog>
      <author keyref="author">
        <keyword>author keyword</keyword>
      </author>
      <source keyref="source">
        <keyword>source keyword</keyword>
      </source>
      <publisher keyref="publisher">
        <keyword>publisher keyword</keyword>
      </publisher>
      <data keyref="data">
        <keyword>data keyword</keyword>
      </data>
      <data-about keyref="data-about">
        <data>data-about data-about</data>
      </data-about>
    </prolog>
    <body>
      <image keyref="image" placement="inline"/>
      <p>
        <xref keyref="xref">
          <keyword>xref keyword</keyword>
        </xref>
        <cite keyref="cite">cite keyword</cite>
        <keyword keyref="keyword">keyword keyword</keyword>
        <ph keyref="ph">ph keyword</ph>
        <term keyref="term">term keyword</term>
        <indexterm keyref="indexterm">indexterm keyword</indexterm>
        <indexterm>
          <index-base keyref="index-base">index-base keyword</index-base>
        </indexterm>
        <indextermref keyref="indextermref">indextermref keyword</indextermref>
      </p>
      <dl>
        <dlentry>
          <dt keyref="dt">dt keyword</dt>
          <dd/>
        </dlentry>
      </dl>
      <lq keyref="lq">
        <keyword>lq keyword</keyword>
      </lq>
      <p>
        <keyword keyref="keyword">original</keyword>
        <keyword keyref="keyword"><text>original</text></keyword>
        <keyword keyref="keyword">keyword keyword</keyword>
        <keyword keyref="keyword"><?original?>keyword keyword</keyword>
      </p>
      <p>
        <xref keyref="missing" scope="external" href="dead" type="dead" format="dead">missing</xref>
        <keyword keyref="missing">missing</keyword>
      </p>
    </body>
    <related-links>
      <link keyref="link">
        <linktext>
          <keyword>link keyword</keyword>
        </linktext>
      </link>
    </related-links>
  </topic>
  <topic id="link">
    <title>With href</title>
    <prolog>
      <author keyref="author_link" scope="external" href="a.xml" type="dead" format="dead">
        <keyword>author keyword</keyword>
      </author>
      <source keyref="source_link" scope="external" href="a.xml" type="dead" format="dead">
        <keyword>source keyword</keyword>
      </source>
      <publisher keyref="publisher_link" scope="external" href="a.xml" type="dead" format="dead">
        <keyword>publisher keyword</keyword>
      </publisher>
      <data keyref="data_link" scope="external" href="a.xml" type="dead" format="dead">
        <keyword>data keyword</keyword>
      </data>
      <data-about keyref="data-about_link" scope="external" href="a.xml" type="dead" format="dead">
        <data>data-about data-about</data>
      </data-about>
    </prolog>
    <body>
      <image keyref="image_link" placement="inline" scope="external" href="b.gif"/>
      <p>
        <xref keyref="xref_link" scope="external" href="a.xml" type="dead" format="dead">
          <keyword>xref keyword</keyword>
        </xref>
        <cite keyref="cite_link">cite keyword</cite>
        <keyword keyref="keyword_link" href="a.xml">keyword keyword</keyword>
        <ph keyref="ph_link">ph keyword</ph>
        <term keyref="term_link" href="a.xml">term keyword</term>
        <indexterm keyref="indexterm_link">indexterm keyword</indexterm>
        <indexterm>
          <index-base keyref="index-base_link">index-base keyword</index-base>
        </indexterm>
        <indextermref keyref="indextermref_link">indextermref keyword</indextermref>
      </p>
      <dl>
        <dlentry>
          <dt keyref="dt_link">dt keyword</dt>
          <dd/>
        </dlentry>
      </dl>
      <lq keyref="lq_link" scope="external" href="a.xml" type="dead" format="dead">
        <keyword>lq keyword</keyword>
      </lq>
      <p>
        <keyword keyref="keyword_link" href="a.xml">original</keyword>
        <keyword keyref="keyword_link" href="a.xml"><text>original</text></keyword>
        <keyword keyref="keyword_link" href="a.xml">keyword keyword</keyword>
        <keyword keyref="keyword_link" href="a.xml"><?original?>keyword keyword</keyword>
      </p>
      <p>
        <xref href="a.xml" keyref="missing" scope="external" type="dead" format="dead">missing</xref>
        <keyword keyref="missing">missing</keyword>
      </p>
    </body>
    <related-links>
      <link keyref="link_link" scope="external" href="a.xml" type="dead" format="dead">
        <linktext>
          <keyword>link keyword</keyword>
        </linktext>
      </link>
    </related-links>
  </topic>
</topic>
