<map xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/" class="- map/map "
  domains="(map mapgroup-d)                            (topic abbrev-d)                            (topic delay-d)                            a(props deliveryTarget)                            (map ditavalref-d)                            (map glossref-d)                            (topic hazard-d)                            (topic hi-d)                            (topic indexing-d)                            (topic markup-d)                            (topic pr-d)                            (topic relmgmt-d)                            (topic sw-d)                            (topic ui-d)                            (topic ut-d)                            (topic markup-d xml-d)   "
  ditaarch:DITAArchVersion="1.3">
  <title class="- topic/title ">Map with Key Scopes</title>
  <topicgroup class="+ map/topicref mapgroup-d/topicgroup " keyscope="dita-europe">
    <keydef class="+ map/topicref mapgroup-d/keydef " keys="conferenceName" processing-role="resource-only">
      <topicmeta class="- map/topicmeta ">
        <keywords class="- topic/keywords ">
          <keyword class="- topic/keyword ">DITA Europe 2013</keyword>
        </keywords>
      </topicmeta>
    </keydef>
    <keydef class="+ map/topicref mapgroup-d/keydef " href="http://www.ihg.com/holidayinn/hotels/us/en/munich/muchb/hoteldetail" keys="hotel" processing-role="resource-only" scope="external">
      <topicmeta class="- map/topicmeta ">
        <linktext class="- map/linktext ">Holiday Inn City Centre</linktext>
      </topicmeta>
    </keydef>
    <keydef class="+ map/topicref mapgroup-d/keydef " format="jpg" href="images/holidayInn.jpg" keys="hotelImage" processing-role="resource-only"/>
    <topicref class="- map/topicref " href="concept.dita"/>
  </topicgroup>
  <topicgroup class="+ map/topicref mapgroup-d/topicgroup " keyscope="telematics">
    <keydef class="+ map/topicref mapgroup-d/keydef " keys="conferenceName" processing-role="resource-only">
      <topicmeta class="- map/topicmeta ">
        <keywords class="- topic/keywords ">
          <keyword class="- topic/keyword ">Telematics Munich 2013</keyword>
        </keywords>
      </topicmeta>
    </keydef>
    <keydef class="+ map/topicref mapgroup-d/keydef " href="http://www.dolcemunich.com/" keys="hotel" processing-role="resource-only" scope="external">
      <topicmeta class="- map/topicmeta ">
        <linktext class="- map/linktext ">Dolce Munich</linktext>
      </topicmeta>
    </keydef>
    <keydef class="+ map/topicref mapgroup-d/keydef " format="jpg" href="images/dolce.jpg" keys="hotelImage" processing-role="resource-only"/>
    <topicref class="- map/topicref " href="concept.dita"/>
  </topicgroup>
</map>
