  <maplinks href="readme/DITA-globalization.xml">
    <linkpool class="- topic/linkpool ">
      <link class="- topic/link " mapclass="+ map/topicref mapgroup-d/mapref " type="concept"
        href="../readme/DITA-readme.xml" role="parent">
        <linktext class="- topic/linktext "><?ditaot gentext?>DITA Open Toolkit User
          Guide</linktext>
        <?ditaot genshortdesc?>
        <desc class="- topic/desc ">The DITA Open Toolkit is an implementation of the OASIS DITA
          Technical Committee's specification for DITA DTDs and Schemas. The Toolkit transforms DITA
          content (maps and topics) into deliverable formats, such as XHTML, PDF, Eclipse Help, HTML
          Help, and JavaHelp.</desc>
      </link>
      <linkpool class="- topic/linkpool ">
        <link class="- topic/link " mapclass="- map/topicref " type="reference"
          href="../readme/DITA-globalization-xhtml.xml" role="child">
          <linktext class="- topic/linktext "><?ditaot gentext?>Localization in XHTML based
            transforms</linktext>
          <?ditaot genshortdesc?>
          <desc class="- topic/desc ">X/HTML based output (such as Eclipse Help, TocJS, and HTML
            Help) is supported for over 50 languages and variants.</desc>
        </link>
        <link class="- topic/link " mapclass="- map/topicref " type="reference"
          href="../readme/DITA-globalization-pdf.xml" role="child">
          <linktext class="- topic/linktext "><?ditaot gentext?>Localization in PDF based
            transforms</linktext>
          <?ditaot genshortdesc?>
          <desc class="- topic/desc ">PDF (PDF2) output is supported for the following
            languages.</desc>
        </link>
      </linkpool>
    </linkpool>
  </maplinks>
  <maplinks href="readme/DITA-globalization-xhtml.xml">
    <linkpool class="- topic/linkpool ">
      <link class="- topic/link " mapclass="- map/topicref " type="concept"
        href="../readme/DITA-globalization.xml" role="parent">
        <linktext class="- topic/linktext "><?ditaot gentext?>Globalization in the
          DITA-OT</linktext>
        <?ditaot genshortdesc?>
        <desc class="- topic/desc ">The DITA standard comes with many features to enable globalized
          content, including the xml:lang, translate, and dir attributes. The DITA-OT uses these
          attributes to help publish content that is written or translated into many different
          languages.</desc>
      </link>
    </linkpool>
