<?xml version="1.0" encoding="UTF-8"?>
<project name="MetadataInheritance" default="all" basedir=".">
  <dirname property="currentdir" file="${ant.file.MetadataInheritance}"/>
  <target name="all">
    <sequential>
      <subant inheritall="true">
        <fileset dir="${currentdir}" includes="${ant.project.name}_*.xml"/>
      </subant>
    </sequential>
  </target>
</project>
