            </xs:sequence>
            <xs:sequence>
                <xs:group ref="month"/>
                <xs:group ref="day" minOccurs="0"/>
                <xs:group ref="year"/>
            </xs:sequence>
            <xs:sequence>
                <xs:group ref="day" />
                <xs:group ref="month"/>
                <xs:group ref="year"/>
            </xs:sequence>
