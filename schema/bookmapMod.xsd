    </xs:element>
    <xs:complexType name="frontmatter.class">
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:group ref="booklists" />
            <xs:group ref="notices" />
            <xs:group ref="dedication" />
            <xs:group ref="colophon" />
