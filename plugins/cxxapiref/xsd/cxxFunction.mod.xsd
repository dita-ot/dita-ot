  <xs:group name="cxxFunctionParameterDeclarationName">
    <xs:sequence>
      <xs:element ref="cxxFunctionParameterDeclarationName"/>
    </xs:sequence>
  </xs:group>
  <xs:group name="cxxFunctionParameterDefinitionName">
    <xs:sequence>
      <xs:element ref="cxxFunctionParameterDefinitionName"/>
