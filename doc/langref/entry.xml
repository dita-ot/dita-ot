      <example>
         <p>The tagging for the following table is shown below:<table>

               
               
               <tgroup cols="2">

                  
                  
                  <tbody>

                     
                     
                     <row>

                        
                        
                        <entry>Asian elephant</entry>

                        
                        
                        <entry>
                           
                           
                           <i>
                              
                              
                              <keyword>Elephas maximus</keyword>
                           
                           
                           </i>
                        
                        
                        </entry>

                     
                     
                     </row>

                     
                     
                     <row>

                        
                        
                        <entry>African elephant (savannah)</entry>

                        
                        
                        <entry>
                           
                           
                           <i>
                              
                              
                              <keyword>Loxodonta africana africana</keyword>
                           
                           
                           </i>
                        
                        
                        </entry>

                     
                     
                     </row>

                     
                     
                     <row>

                        
                        
                        <entry>African elephant (forest)</entry>

                        
                        
                        <entry>
                           
                           
                           <i>
                              
                              
                              <keyword>Loxodonta africana cyclotis</keyword>
                           
                           
                           </i>
                        
                        
                        </entry>

                     
                     
                     </row>

                  
                  
                  </tbody>

               
               
               </tgroup>

            
            
            </table>
         
         
         </p>
         <codeblock>&lt;table&gt;
&lt;tgroup cols="2"&gt;&lt;colspec colname="col1"/&gt;&lt;colspec colname="col2"/&gt;
&lt;tbody&gt; 
&lt;row&gt;&lt;entry colname="col1"&gt;Asian elephant&lt;/entry&gt;
&lt;entry colname="col2"&gt;&lt;i&gt;Elephas maximus&lt;/i&gt;&lt;/entry&gt; &lt;/row&gt; 
&lt;row&gt;&lt;entry colname="col1"&gt;African elephant (savannah)&lt;/entry&gt; 
&lt;entry colname="col2"&gt;&lt;i&gt;Loxodonta africana africana&lt;/i&gt;&lt;/entry&gt;&lt;/row&gt; 
&lt;row&gt; &lt;entry colname="col1"&gt;African elephant (forest)&lt;/entry&gt;
&lt;entry colname="col2"&gt;&lt;i&gt;Loxodonta africana cyclotis&lt;/i&gt;&lt;/entry&gt;
&lt;/row&gt; &lt;/tbody&gt; &lt;/tgroup&gt; &lt;/table&gt;</codeblock>
